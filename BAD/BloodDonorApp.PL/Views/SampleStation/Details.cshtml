@model BloodDonorApp.BL.EF.DTO.SampleStationDto

@{
    ViewBag.Title = "Details";
}
<div class="container body-content">
    <h2>Details</h2>
    <br />
    <div>
        <hr />
        <dl class="dl-horizontal" style="font-size:100%">
            <dt>
                @Html.DisplayNameFor(model => model.City):
            </dt>

            <dd>
                @Html.DisplayFor(model => model.City)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Street):
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Street)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Name):
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.WebLink):
            </dt>

            <dd>
                <a href="@Model.WebLink">@Model.WebLink</a>
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.PhoneNumber):
            </dt>

            <dd>
                @Html.DisplayFor(model => model.PhoneNumber)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Email):
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Email)
            </dd>
        </dl>
        <hr />
    </div>
    <button type="button" class="boxed-btn" title="Browse sample stations" onclick="location.href='@Url.Action("Index", "SampleStation")'">Back to List</button>
</div>
